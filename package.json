{
  "name": "fcl-frtl-wix",
  "version": "1.0.0",
  "description": "Frtl Creative Labs website with e-commerce marketplace and crypto payments",
  "main": "index.js",
  "scripts": {
    "dev": "wix-cli dev",
    "build": "wix-cli build",
    "test": "jest",
    "wix:preview": "wix-cli site:preview --siteId $WIX_SITE_ID",
    "wix:publish": "wix-cli site:publish --siteId $WIX_SITE_ID",
    "wix:login": "wix-cli login --apiKey $WIX_CLI_API_KEY",
    "deploy:preview": "npm run build && npm run wix:preview",
    "deploy:production": "npm run build && npm run wix:publish"
  },
  "keywords": [
    "wix",
    "velo",
    "ecommerce",
    "stripe",
    "crypto",
    "blockchain",
    "marketplace"
  ],
  "author": "Frtl Creative Labs",
  "license": "MIT",
  "dependencies": {
    "stripe": "^14.0.0",
    "crypto": "^1.0.1",
    "axios": "^1.6.0",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "@wix/cli": "^1.0.0",
    "jest": "^29.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/frtl-creative-labs/fcl-frtl-wix.git"
  },
  "bugs": {
    "url": "https://github.com/frtl-creative-labs/fcl-frtl-wix/issues"
  },
  "homepage": "https://github.com/frtl-creative-labs/fcl-frtl-wix#readme"
}
